# QJury Quantum Oracle Configuration
# Copy this file to .env and fill in your values

# Ethereum Network Configuration
RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY
# For testing: RPC_URL=http://localhost:8545

# Oracle Private Key (for signing transactions)
PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# Smart Contract Addresses
ORACLE_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890

# Optional: Etherscan URL for transaction links
ETHERSCAN_URL=https://etherscan.io
# For testnets: ETHERSCAN_URL=https://goerli.etherscan.io

# Optional: Gas settings
GAS_LIMIT=300000
GAS_PRICE=20000000000

# Optional: Network configuration
CHAIN_ID=1
# For testnets: CHAIN_ID=5 (Goerli), CHAIN_ID=11155111 (Sepolia)

# Optional: Logging configuration
LOG_LEVEL=info
ENABLE_DEBUG=false 